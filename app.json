{
  "name": "Notion2Instagram",
  "description": "Publish posts to Instagram from Notion",
  "repository": "https://github.com/yudax42/notion-instagram",
  "logo": "https://raw.githubusercontent.com/yudax42/notion-instagram/master/thumbnail.png",
  "keywords": ["Notion", "Instagram"],
  "env": {
    "IG_USERNAME": {
      "description": "Instagram account username",
      "required": true
    },
    "IG_PASSWORD": {
      "description": "Instagram account password",
      "required": true
    },
    "NT_DB": {
      "description": "Notion Database ID",
      "required": true
    },
    "NT_SECRET": {
      "description": "Notion integration secret",
      "required": true
    },
    "NT_TOKEN": {
      "description": "token_v2 value (check the documentation to know how find it)",
      "required": false
    },
    "INTERVAL": {
      "description": "Interval to check new posts on Notion in ms default is 15s (15000 ms)",
      "required": false
    }
  }
}
